{
    "name": "acid21/somfy",
    "type": "project",
    "license": "proprietary",
    "description": "",
    "minimum-stability": "alpha",
    "prefer-stable": true,
    "require": {
        "php": "8.0.*",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "danaki/doctrine-enum-type-bundle": "^0.2",
        "doctrine/doctrine-bundle": "^2.4",
        "doctrine/doctrine-migrations-bundle": "^3.1",
        "doctrine/orm": "^2.9",
        "friendsofsymfony/ckeditor-bundle": "^2.3",
        "friendsofsymfony/jsrouting-bundle": "^3.1",
        "gedmo/doctrine-extensions": "^3.1",
        "helios-ag/fm-elfinder-bundle": "^12.1",
        "intervention/image": "^2.6",
        "knplabs/doctrine-behaviors": "^2.3",
        "myclabs/php-enum": "^1.8",
        "nelmio/api-doc-bundle": "^4.0",
        "nelmio/cors-bundle": "^2.1",
        "nesbot/carbon": "^2.50",
        "sonata-project/admin-bundle": "^4.8",
        "sonata-project/doctrine-orm-admin-bundle": "^4.2",
        "sonata-project/google-authenticator": "^2.3",
        "symfony/console": "*",
        "symfony/dotenv": "*",
        "symfony/flex": "*",
        "symfony/framework-bundle": "*",
        "symfony/mailer": "*",
        "symfony/messenger": "*",
        "symfony/monolog-bundle": "*",
        "symfony/runtime": "*",
        "symfony/security-bundle": "*",
        "symfony/serializer": "*",
        "symfony/translation": "*",
        "symfony/twig-bundle": "*",
        "symfony/uid": "*",
        "symfony/validator": "*",
        "symfony/web-link": "*",
        "symfony/webpack-encore-bundle": "*",
        "symfony/yaml": "*",
        "vich/uploader-bundle": "^1.18"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "friendsofphp/php-cs-fixer": "^3.0",
        "roave/security-advisories": "dev-latest",
        "symfony/maker-bundle": "*",
        "symfony/stopwatch": "*",
        "symfony/web-profiler-bundle": "*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear --no-warmup": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "install-editor": {
            "ckeditor:install --exclude=samples --exclude=adapters --clear=drop": "symfony-cmd",
            "elfinder:install": "symfony-cmd",
            "asset:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.4.*"
        }
    }
}
